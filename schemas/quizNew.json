{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "additionalProperties": false,
    "required": [
        "title",
        "description",
        "creator"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Shapes Quiz"
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "Geometry quiz about shapes."
            ]
        },
        "isPublic": {
            "type": "boolean",
            "default": false,
            "title": "The isPublic Schema",
            "examples": [
                false
            ]
        },
        "creator": {
            "type": "string",
            "default": "",
            "title": "The creator Schema",
            "examples": [
                "username"
            ]
        }
    },
    "examples": [
        {
            "title": "Shapes Quiz",
            "description": "Geometry quiz about shapes.",
            "isPublic": false,
            "creator": "username"
        }
    ]
}